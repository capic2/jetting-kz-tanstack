// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Settings {
  engineType String @unique
  sparkleType String @unique
  emulsionTubeType String @unique
  floatType String @unique
  floatHeight Int @unique
  airbox String @unique
  semiStepNeedle Boolean @unique
  oilMix Int @unique
  fuelType String @unique
  trackType String @unique
}
